@base <http://www.oecd.com/oecd> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix odrl: 	 <http://www.w3.org/ns/odrl/2/> .
@prefix dct:     <http://purl.org/dc/terms/> .
@prefix odrlprof:<http://oecd.com/odrl:profile/odrlprof> .
@prefix oecdns: <http://oecd.com/ns/oecd> .
@prefix oecd:    <http://oecd.com/oecd/> .

oecd: a dct:Policy .

<http://oecd.com/oecd/policy/1.1> a odrl:Policy, odrl:Set ;
  	oecdns:Recommendation [
  	
    odrl:party oecdns:members, oecdns:non-members, oecdns:adherents;
    odrl:action oecdns:promote;
    odrl:action oecdns:implement;
    odrl:target <http://www.oecd.com/oecd> 
  						];